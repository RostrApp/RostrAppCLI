	"item": [
		{
			"name": "Init",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"let data = pm.response.json();\r",
							"\r",
							"// Success case\r",
							"if (pm.response.code === 200) {\r",
							"    pm.test(\"Init returns 201 Created\", function () {\r",
							"        pm.response.to.have.status(200);\r",
							"    });\r",
							"    pm.test(\"Init success message present\", function () {\r",
							"        pm.expect(data).to.have.property(\"message\");\r",
							"        pm.expect(data.message).to.include(\"db initialized!\");\r",
							"    });\r",
							"} else {\r",
							"    // Fail case\r",
							"    pm.test(\"Init failed with error\", function () {\r",
							"        pm.expect(pm.response.code).to.be.oneOf([400, 500]);\r",
							"    });\r",
							"    pm.test(\"Error message present\", function () {\r",
							"        pm.expect(data).to.have.property(\"error\");\r",
							"    });\r",
							"    console.log(\"Init failed: \" + (data.error || \"Unknown error\"));\r",
							"}\r",
							""
						],
						"type": "text/javascript",
						"packages": {},
						"requests": {}
					}
				}
			],
			"request": {
				"method": "GET",
